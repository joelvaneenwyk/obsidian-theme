{
  "name": "mycoshiro-theme",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "src/index.scss",
  "packageManager": "yarn@3.6.1",
  "author": "Joel Van Eenwyk",
  "authorUrl": "https://github.com/joelvaneenwyk/",
  "repository": {
    "type": "git",
    "url": "https://github.com/joelvaneenwyk/mycoshiro-theme.git"
  },
  "scripts": {
    "dev": "node esbuild.config.mjs dev",
    "build": "node esbuild.config.mjs production",
    "deploy": "yarn deploy:css && yarn deploy:manifest",
    "deploy:css": "cross-env-shell \"yarn shx --verbose cp dist/main.min.css \"${PROJECT_CWD}/.obsidian/themes/Mycoshiro/theme.css\" \"",
    "deploy:manifest": "cross-env-shell \"yarn shx --verbose cp manifest.json \"${PROJECT_CWD}/.obsidian/themes/Mycoshiro\" \"",
    "release": "yarn build && yarn deploy"
  },
  "dependencies": {
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@yarnpkg/sdks": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-plugin-import-glob": "^2.0.0",
    "builtin-modules": "^3.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.11",
    "esbuild-plugin-import-glob": "^0.1.1",
    "esbuild-plugin-replace": "^1.4.0",
    "esbuild-sass-plugin": "^2.16.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-json-files": "^4.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^2.1.2",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-unicorn": "^50.0.1",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.1",
    "sass": "^1.69.7",
    "shx": "^0.3.4",
    "svelte": "^4.2.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "unified-lint-rule": "^2.1.2",
    "vue-eslint-parser": "^9.3.2"
  }
}
